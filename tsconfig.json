{
  "compilerOptions": {
      "module": "commonjs",
      "target": "es5",
      "removeComments": true,
      "noImplicitAny": true,
      "preserveConstEnums": true,
      "sourceMap":true,
      "outDir": "build",
      "types": ["node", "jest", "chance"],
      "paths": {
        "@models/*": ["./src/models/*"],
        "@utils/*": ["./src/utils/*"]
      },
  },
  "include": ["src/*"],
  "exclude": [],
}